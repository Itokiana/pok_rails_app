function getAllTeam(){
  let base_url = $("#base_url").val()
  let team = $("#team")
  $.ajax({
    type: 'get',
    url: base_url + '/teams/<%= @team.id %>/show',
    success: (res) => {
      $("#team_name").val('')
      $("#team_empty").remove()
      team.append(`

        <tr id="team${res.id}">
          <td>${res.name}</td>
          <td class="text-center">
            <%= link_to delete_team_path(@team.id), class:"btn btn-danger btn-circle", method: :delete, data: {confirm: "Êtes-vous sûre de vouloir supprimer?"}, remote: true do %>
              <i class="fas fa-trash"></i>
            <% end %>
          </td>
        </tr>
      
      `)
    }
  })
}

getAllTeam()